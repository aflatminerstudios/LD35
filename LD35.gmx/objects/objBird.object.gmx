            <string>///Start flying away

if (!leaving) {
  leaving = true;
  var r = irandom(4);
  
  switch (r) {
    case 0:
      targetX = irandom(room_width);
      targetY = -25;
      break;
    case 1:
      targetX = room_width + 25;
      targetY = irandom(room_height);
      break;

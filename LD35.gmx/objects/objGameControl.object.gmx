            <kind>1</kind>
            <string>///Spawn new picture or end game, also score



global.pcts[global.numComplete] = scrGetPicturePercent(pic);
global.times[global.numComplete] = timer;

var p = global.pcts[global.numComplete];

global.numComplete++;

if (p &gt;= 100) {
  audio_play_sound(sndFarmer_Next_Pic_Perfect, 0, false);
} else if (p &gt;= 95) {
  audio_play_sound(sndFarmer_Next_Pic_Amazing, 0, false);
} else if (p &gt;= 92) {
  audio_play_sound(sndFarmer_Next_Pic_Great, 0, false);
} else if (p &gt;= 85) {
  audio_play_sound(sndFarmer_Next_Pic_Good, 0, false);
} else if (p &gt;= 75) {
  audio_play_sound(sndFarmer_Next_Pic_Fair, 0, false);
} else if (p &gt;= 60) {
  audio_play_sound(sndFarmer_Next_Pic_Kinda_Bad, 0, false);
} else {
  audio_play_sound(sndFarmer_Next_Pic_Bad, 0, false);
}




with (pic) {
  instance_destroy();
}

if (howManyPics &lt; (picsPerGame)) {
  scrSpawnPic(self.id);
} else {
  scrEndGame();
}

ending = false;
